{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "semi": "warn",
        "comma-dangle": "warn",
        "quotes": [
            "warn",
            "double"
        ]
    }
}